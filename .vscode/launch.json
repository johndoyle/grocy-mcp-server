{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run MCP SSH Client",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dev/mcp-ssh-client.js",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "MCP_SSH_ARGS": "[\"john@192.168.24.24\",\"docker\",\"exec\",\"-i\",\"grocy-mcp-server\",\"node\",\"build/index.js\"]"
      }
    }
  ]
}
